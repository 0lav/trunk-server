;\n    var errorMessage = \"\";\n\n    if (this.state.error) {\n      errorMessage = (\n      <Message negative>\n        <Message.Header>That didn't work...</Message.Header>\n        <p>{this.state.error}</p>\n      </Message>\n      )\n    }\n\n    return (\n      <Modal size='tiny' open={this.props.open} onClose={this.handleClose} centered={false}>\n        <Modal.Header>Select Role</Modal.Header>\n        <Modal.Content >\n          <Modal.Description>\n          <Form>\n       <Form.Field>\n         Add User Access to System: {this.props.shortName}\n       </Form.Field>\n       <Form.Field>\n         <Input label=\"User's email\" placeholder='user@domain.com' name='email' onChange={this.handleInputChange} />\n       </Form.Field>\n       <Form.Field>\n         <Checkbox\n           radio\n           label='User'\n           name='role'\n           value={5}\n           checked={(this.state.role >= 0) && (this.state.role < 10)}\n           onChange={this.handleInputChange}\n         />\n       </Form.Field>\n       <Form.Field>\n         <Checkbox\n           radio\n           label='Admin'\n           name='role'\n           value={15}\n           checked={(this.state.role >= 10) && (this.state.role < 20)}\n           onChange={this.handleInputChange}\n         />\n       </Form.Field>\n     </Form>\n     {errorMessage}\n    </Modal.Description>\n  </Modal.Content>\n  <Modal.Actions>\n    <Button onClick={this.handleClose} >\n      <Icon name='remove' /> Cancel\n    </Button>\n    <Button onClick={this.handleSubmit} >\n      <Icon name='plus' /> Add\n    </Button>\n  </Modal.Actions>\n</Modal>\n\n    );\n  }\n}\n\nexport default AddPermissionModal;\n","/Users/luke/Projects/OpenMHz/trunk-server-public/admin/src/Group/GroupModalContainer.js",[],"/Users/luke/Projects/OpenMHz/trunk-server-public/admin/src/System/ListSystemsContainer.js",[],"/Users/luke/Projects/OpenMHz/trunk-server-public/admin/src/Permission/UpdatePermissionModal.js",["236","237","238","239","240"